<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>CountryAlphabetic_Value</fullName>
    <active>true</active>
    <errorConditionFormula>NOT(REGEX( BillingCountry ,&quot;^[a-z A-Z]*$&quot;))</errorConditionFormula>
    <errorMessage>Please enter alphabetic character in the Country field.</errorMessage>
</ValidationRule>
