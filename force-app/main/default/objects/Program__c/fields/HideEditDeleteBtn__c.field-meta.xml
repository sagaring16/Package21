<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>HideEditDeleteBtn__c</fullName>
    <description>AND((TEXT(Status__c) != &apos;Created&apos;), (TEXT(Status__c) != &apos;Being Amended&apos;))</description>
    <formula>NOT(
AND(
OR(
TEXT(Status__c) = &apos;Created&apos;,TEXT(Status__c) = &apos;Being Amended&apos;), 
$User.Id = OwnerId)
)</formula>
